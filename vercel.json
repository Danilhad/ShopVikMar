{
  "version": 2,
  "builds": [
    {
      "src": "vite.config.ts",
      "use": "@vite/vercel"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "methods": ["GET"],
      "headers": {
        "X-Frame-Options": "ALLOWALL",
        "Content-Security-Policy": "frame-ancestors https://*.telegram.org"
      }
    }
  ]
}